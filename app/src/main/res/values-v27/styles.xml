<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/tabLayoutColor</item>
        <item name="colorPrimaryDark">@color/tabLayoutColor</item>
        <item name="colorAccent">@color/Indigo</item>
        <item name="android:navigationBarColor">@color/tabLayoutColor</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="android:windowLightStatusBar">true</item>

    </style>


    <style name="LibAppTheme" parent="Theme.AppCompat.Light">

        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

</resources>